generator client {
   provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}
 
 model Router {
   id          String   @id @default(cuid())
   name        String
   host        String
   port        Int      @default(8728)
   username    String
   password    String
   hotspotName String?
   dnsName     String?
   currency    String   @default("Rp")
   autoReload  Int      @default(10)
   createdAt   DateTime @default(now())
   updatedAt   DateTime @updatedAt
 
   @@map("routers")
 }
 
 model AdminConfig {
   id       Int    @id @default(autoincrement())
   username String
   password String
 
   @@map("admin_config")
 }
